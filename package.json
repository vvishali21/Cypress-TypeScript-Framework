  "license": "ISC",
  "dependencies": {
    "cypress-parallel": "^0.12.0",
    "typescript": "^5.0.3"
  },
  "devDependencies": {
    "cypress": "^12.10.0",
    "cypress-iframe": "^1.0.1",
    "cypress-mochawesome-reporter": "^3.3.0",
    "cypress-xpath": "^2.0.1",
    "lambdatest-cypress-cli": "^3.0.12",
    "mochawesome-merge": "^4.3.0"
  }
